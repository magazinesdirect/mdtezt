{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://web:wide4ever@newstaging.magazinesdirect.widearea.co.uk/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='column-content']//a[.='Buy now']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='pricing-block']/table[1]/tbody/tr[2]/td[5]/form/button"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "checkout"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "billing-title"
      },
      "text": "Dr"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "billing-first-name"
      },
      "text": "Simon"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "billing-surname"
      },
      "text": "Brock"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "billing-email"
      },
      "text": "mdtest@wideareaprojects.co.uk"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "billing-postcode"
      },
      "text": "w1w 6qq"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "button-find-address-billing"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "button-use-address-billing"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "card-number"
      },
      "text": "4539123412341234"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//div[@id='payment-details']/fieldset/ol/li[4]/select[2]/option[3]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "continue"
      }
    }
  ]
}